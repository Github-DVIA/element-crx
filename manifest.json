{
  "manifest_version": 3,
  "name": "Element",
  "version": "1.0",
  "icons": {
    "48": "vector-icons/50.png",
    "50": "vector-icons/50.png",
    "120": "vector-icons/120.png",
    "128": "vector-icons/150.png",
    "180": "vector-icons/180.png",
    "300": "vector-icons/300.png"
  },
  "action": {
    "default_icon": {
      "48": "vector-icons/50.png",
      "50": "vector-icons/50.png",
      "120": "vector-icons/120.png",
      "128": "vector-icons/150.png",
      "180": "vector-icons/180.png",
      "300": "vector-icons/300.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "index.html",
  "background": {
    "service_worker": "sw.js"
  },
  "permissions": ["unlimitedStorage"]
}
